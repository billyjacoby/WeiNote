<%- include('header')%>

<div class="dairy" style="margin-top: 70px;">
  <div class="dairy-p markdown-body" style="width: 50%!important; display: block; margin:auto;">
    <div class="markdown-status-bar">
        <span style="margin: auto auto auto auto;"> </span>
    </div>
    <div class="markdown-content">

      <form class="create-form" method="post">
          <div class="form-item" style="margin: 0 auto 0 0;">
              <span class="txtlabel">Username</span>
              <input class="input-bar" name="name" type="text" >
          </div>
          <div class="form-item">
              <span class="txtlabel">Password</span>
              <input class="input-bar" name="password" type="password">
          </div>
          <div class="form-item" style="margin: 20px 0">
              <input type="submit" class="login-bnt" value="Login In">
          </div>
      </form>

    </div>
  </div>
</div>

<%- include('footer') %>